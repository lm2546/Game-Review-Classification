  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "ddefeed0-c180-4148-b9e5-22d5c16616d8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def preprocess(str_arg):\n",
    "        cleaned_str=re.sub('[^a-z\\s]+',' ',str(str_arg), flags=re.IGNORECASE) #every char except alphabets is replaced\n",
    "        cleaned_str=re.sub('(\\s+)',' ',cleaned_str) #multiple spaces are replaced by single space\n",
    "        cleaned_str=cleaned_str.lower() #converting the cleaned string to lower case\n",
    "        \n",
    "        return cleaned_str # returning the preprocessed string"
   ]
  },
